<% if @ranked %>
	<script src="/static/pongsocket-ranked.js"></script>
<% else %>
	<script src="/static/pongsocket-normal.js"></script>
<% end %>
<div>
	<div style="display: flex; justify-content: center;">
		<h1 id="game_name">Pongpong<br></h1>
	</div>
	<div style="display: flex; justify-content: center;">
		<h2>Score</h2>
	</div>
	<div style="display: flex; justify-content: center;">
		<h2 id="score_text">0 - 0</h2>
	</div>
	<div style="display: flex; align-items: center; justify-content: center;">
		<canvas id="drawCanvas" width="800px" height="600px"
			style="display: flex; align-items: center; justify-content: center;"></canvas>
	</div>
	<div style="display: flex; justify-content: center;">
		<h1 id="winner_text"></h1>
	</div>
</div>